syntax = "v1"

info(
	title: "type title here"
	desc: "type desc here"
	author: "type author here"
	email: "type email here"
	version: "type version here"
)

type Request {
	Sub string `json:"subt"`
	Obj string `json:"obj"`
	Act string `json:"act"`
}

type Response {
	Message string `json:"message"`
}

@server(
	group: api
	prefix: /v1
)
service api {
	@handler helloHandler
	get /hello (Request) returns (Response)
}